{
  "name": "gitbook-plugin-woopra",
  "version": "1.0.2",
  "engines": {
        "gitbook": ">=2.5.1"
    },
  "description": "A GitBook plugin for Woopra analytics",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianshumate/gitbook-plugin-woopra.git"
  },
  "keywords": [
    "GitBook",
    "Woopra"
  ],
  "author": "Brian Shumate",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/brianshumate/gitbook-plugin-woopra/issues"
  },
  "homepage": "https://github.com/brianshumate/gitbook-plugin-woopra#readme",
  "gitbook": {
        "properties": {
            "domain": {
                "type": "string",
                "title": "Domain name",
                "required": true
            },
            "idle_timeout": {
                "type": "integer",
                "title": "Idle timeout",
                "required": true
            },
            "configuration": {
                "type": [ "string", "object" ],
                "title": "Configuration mode",
                "default": "auto"
            }
        }
    }
}
